{
  "name": "@sardonyxwt/ligui",
  "version": "7.3.7",
  "description": "Ligui",
  "keywords": [
    "sclib",
    "ligui",
    "ui"
  ],
  "license": "MIT",
  "author": "sardonyx.wt@gmail.com",
  "contributors": [
    "Kirill Petkov <sardonyx.wt@gmail.com>"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sardonyxwt/ligui.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "webpack --mode production",
    "test": "cross-env-shell webpack -p && jest",
    "lint": "tslint -p . -c tslint.json --fix -t verbose",
    "deploy": "cross-env-shell npm run build && npm publish --access public"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "/(test|src)/.*\\.(test|spec)\\.(ts|js)$"
  },
  "dependencies": {
    "@sardonyxwt/event-bus": "1.2.0",
    "@sardonyxwt/state-store": "5.0.2",
    "@sardonyxwt/utils": "2.1.1",
    "autobind-decorator": "^2.4.0",
    "inversify": "^5.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "parallel-webpack": "^2.3.0",
    "stats-webpack-plugin": "^0.7.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
