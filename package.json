{
  "name": "@sardonyxwt/ligui",
  "version": "5.2.2",
  "description": "Ligui",
  "main": "dist/ligui.min.js",
  "typings": "dist/@types/index.d.ts",
  "keywords": [
    "sclib",
    "ligui",
    "ui"
  ],
  "license": "MIT",
  "author": "sardonyx.wt@gmail.com",
  "contributors": [
    "Kirill Petkov <sardonyx.wt@gmail.com>"
  ],
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sardonyxwt/ligui.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "webpack --mode production",
    "test": "cross-env-shell webpack && jest",
    "lint": "tslint -p . -c tslint.json --fix -t verbose",
    "deploy": "cross-env-shell npm run build && npm publish --access public"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testRegex": "/(test|src)/.*\\.(test|spec)\\.(ts|js)$"
  },
  "dependencies": {
    "@sardonyxwt/state-store": "4.4.0",
    "@sardonyxwt/utils": "^1.6.4",
    "inversify": "^5.0.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "@types/node": "^9.6.35",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "@types/react-router": "^4.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "5.1.1",
    "jest": "22.4.4",
    "parallel-webpack": "^2.3.0",
    "stats-webpack-plugin": "^0.6.2",
    "ts-jest": "22.4.6",
    "ts-loader": "4.5.0",
    "tslint": "5.11.0",
    "typescript": "3.1.3",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
